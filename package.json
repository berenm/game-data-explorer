{
  "name": "game-data-explorer",
  "version": "0.0.8",
  "description": "Explore video game engines assets using Atom",
  "main": "./lib/game-data-explorer",
  "dependencies": {
    "atom": "^1.1.0",
    "atom-space-pen-views": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babylonjs": "^3.1.0-alpha2",
    "binary-parser": "^1.1.5",
    "bluebird": "^0.11.6",
    "etch": "^0.12.6",
    "event-kit": "^2.4.0",
    "fs-plus": "^2",
    "humanize-plus": "~1.4.2",
    "istextorbinary": "^2.1.0",
    "jison": "^0.4.18",
    "loophole": "^0",
    "lzo": "^0",
    "serializable": "^1",
    "temp": "~0.8.1",
    "underscore-plus": "^1.6.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "prettier": "^1.6.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-promise": "^3.5.0"
  },
  "repository": "https://github.com/berenm/game-data-explorer",
  "license": "UNLICENSE",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "scripts": {
    "postinstall": "apm rebuild"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:promise/recommended"
    ],
    "plugins": [
      "prettier",
      "react",
      "promise"
    ],
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true
      }
    },
    "rules": {
      "no-console": 0,
      "react/react-in-jsx-scope": 0,
      "react/no-string-refs": 0,
      "promise/avoid-new": 0
    }
  }
}
